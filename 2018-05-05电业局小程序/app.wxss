/*主要线 start*/
.t-line:before{position: absolute;top:0; left: 0px; content:"";border-top: 1px solid #edeef1; transform-origin:0 0;transform:scaleY(1); width: 100%; height: 1px;-webkit-transform:scaleY(1); z-index: 2}/* 头部线 */
.b-line:after{position: absolute;bottom:0; left: 0px; content:""; border-bottom: 1px solid #edeef1; transform-origin:0 100%;transform:scaleY(1); width: 100%; height: 1px;-webkit-transform:scaleY(1);z-index: 2} /*底部线 */
 .r-line:before{position: absolute;right:0; top: 0px; content:""; border-right: 1px solid #edeef1; transform-origin:100% 0;transform:scaleY(1); height: 100%; width: 1px;-webkit-transform:scaleY(1);z-index: 2} /*右边线 */
/*主要线 end*/

/* tab start */
.mian-tab-ul{height: 40px; line-height: 40px; background: #fff; border-bottom: 1px solid #d2d2d2}
.mian-tab-ul view text{display: inline-block; font-size: 14px; color: #666; padding: 0 20rpx}
.mian-tab-ul view.active text{color: #e44a37; border-bottom: 2px solid #e44a37; line-height: 36px;}
/* tab end */


/* tab start */
.ss-nav{background: #fff;z-index: 10;height: 40px;}
.ss-nav-item text{padding: 10px 5px; display: inline-block; overflow: hidden;text-overflow:ellipsis;white-space: nowrap;max-width:60px }
.ss-nav-item.on text{color: #00a5ff; border-bottom: 2px solid #00a5ff;height: 17px}
/* tab end */

