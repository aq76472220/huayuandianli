/* pages/index/index.wxss */
.ss-index-banner{height: 200rpx; background: #00a5ff url('data:image/png;base64,') no-repeat center bottom; background-size:100%;  padding: 30rpx 30rpx;}
.ss-index-banner-top .img{background: rgba(255, 255, 255, 0.5); width: 120rpx; height: 120rpx; overflow: hidden; border-radius: 100px;}
.ss-index-banner-top .img image{display: block; width: 120rpx; height: 120rpx;}

/* 公告 */
.swiper-adv {padding: 15px 10px; background: #fff; height: 20px; padding-left: 40px; padding-right: 0px;} 
.swiper-adv-item {height: 25px;}
.swiper-adv-licon image{display: block; width: 22px; height: 22px;}
.swiper-adv-font{display: inline-block; height: 30px; width: 30px; background: url('data:image/png;base64,') no-repeat right center; background-size: 15px; padding-right: 15px;}

/* 菜单 */
/* .ss-index-menu-box{background: #fff} */
.ss-index-menu-item{background: #fff; height: 250rpx;}
.ss-index-menu-item image{display: block; width: 50px; height: 50px;}
.ss-index-menu-item .con{position: absolute; left: 50%; top: 50%; transform: translate(-50%,-50%)}
.ss-index-menu-item .con .img{width: 50px; height: 50px; margin: 0 auto}
.ss-index-menu-item .con text{width: 100%;}
.ss-index-menu-item:nth-child(3n)::before{display: none}

.ss-index-smbtn{width: 160px; height: 50px; background: #00a5ff; margin: 0 auto; text-align: center; line-height: 50px; border-radius: 100px;box-shadow: 0px 5px 10px 0px rgba(184, 229, 255, 0.8);}
.ss-index-smbtn text{text-align: center; font-size: 16px; color: #fff;padding: 10px 0;  padding-left: 30px; background: url('data:image/png;base64,') no-repeat left center; background-size: 20px;}

/* 弹框 */
.ss-index-punch .tit{text-align: center}
.ss-index-punch .tit text{padding: 10px 0; padding-left: 33px; background: url(data:image/png;base64,) no-repeat left center; background-size: 27px;}
.ss-index-punch .con{text-align: center;}
.ss-index-punch .con>view{text-align: center;display: inline-block;padding: 0 5px 0 5px; border-bottom: 1px solid #00a5ff}
.ss-index-punch .con>view>view{background: #00a5ff; color: #fff; font-size: 17px; padding: 15px;border-top-left-radius:10px;border-top-right-radius:10px;}
.ss-index-logo{padding-top: 10px; padding-bottom: 20px;}
.ss-index-logo image{display: block; width: 285px; height: 45px; margin: 0 auto}